    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@testing-library/cypress": "^10.0.2",
