{
  "name": "nextjs-typescript-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:ci": "vitest run",
    "test:watch": "vitest watch --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json|md)\" --write",
    "lint": "eslint --ext js,jsx,ts,tsx --fix --ignore-path .gitignore .",
    "stylelint": "stylelint **/*.{css,scss} --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@vercel/nft": "^0.26.4",
    "next": "14.1.3",
    "prisma": "^5.10.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@storybook/addon-actions": "^7.6.16",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-docs": "^7.6.16",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "20.11.25",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "1.3.1",
    "autoprefixer": "^10.4.18",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.6",
    "cypress-vite": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.11",
    "jest-axe": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.3",
    "msw-storybook-addon": "^1.10.0",
    "postcss": "^8.4.35",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prettier-stylelint": "^0.4.2",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.17",
    "stylelint": "^16.2.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "vitest-axe": "^0.1.0",
    "zod": "^3.21.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=21.5.0",
    "pnpm": ">=8.15.0"
  }
}
