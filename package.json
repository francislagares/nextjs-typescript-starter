{
  "name": "nextjs-typescript-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:test": "NODE_ENV=\"test\" && next build",
    "start": "next start",
    "start:test": "NODE_ENV=\"test\" && next start",
    "test:ci": "vitest run",
    "test:watch": "vitest watch --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress:start": "start-server-and-test 'yarn start:test' 3000 'yarn cypress:open'",
    "cypress:build": "yarn build:test && yarn cypress:start",
    "cypress:run:chrome": "start-server-and-test 'yarn start:test' 3000 'env-cmd -f .env.test.local cypress run --browser chrome'",
    "cypress:run:firefox": "start-server-and-test 'yarn start:test' 3000 'env-cmd -f .env.test.local cypress run --browser firefox'",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json|md)\" --write",
    "stylelint": "stylelint **/*.{css,scss,tsx,js} --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@vercel/nft": "^0.23.1",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/blocks": "^7.4.1",
    "@storybook/nextjs": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-vite": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "commitizen": "^4.3.0",
    "cypress": "^13.2.0",
    "cypress-vite": "^1.4.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.1",
    "msw-storybook-addon": "^1.8.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prettier-stylelint": "^0.4.2",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.4.1",
    "stylelint": "^15.10.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.6.3"
}
